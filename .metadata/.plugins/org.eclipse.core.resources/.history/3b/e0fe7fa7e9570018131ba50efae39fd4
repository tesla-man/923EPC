#include "term.h"


void termDmmSplash(void){
    printf("\033[2J");       //clear terminal
    printf("\033[H");        //home cursor
    printf("\033)");
    printf("\033[%dm", 44); //set cyan bg
    printf("\033[1;%dm", 37); //set cyan text
    printf("\033[9mbold");

}

