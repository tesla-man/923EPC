#include "msp.h"
#include "interrupt.h"
#include "gpio.h"
#include "spi.h"
#include "uart.h"
#include "delay.h"


int main(void)
{
    uint16_t data = 0;
    initClock();
    spiInit();
    uartInit();

    // Enable global interrupt
    __enable_irq();


    while(1){
        if(checkCR()=data){
            driveDAC(data);
        }
    }
}
