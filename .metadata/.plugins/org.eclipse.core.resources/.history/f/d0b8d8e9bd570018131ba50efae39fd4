#include "msp.h"
#include <stdio.h>
#include <stdlib.h>
#include <stddef.h>
#include <string.h>
#include <stdint.h>
#include "uart.h"
#include "delay.h"


int fputc(int _c, register FILE *_fp);
int fputs(const char *_ptr, register FILE *_fp);

//static FILE mystdout = FDEV_SETUP_STREAM(usartPutcharPrintf, NULL, _FDEV_SETUP_WRITE);

int main(void)
{
    initClock();
    uartWriteByte(0x41);
    printf("hello.");
    while(1){
        uartWriteByte(0x41);
    }
}


int fputc(int _c, register FILE *_fp)
{
  uartWriteByte((unsigned char) _c);

  return((unsigned char)_c);
}

int fputs(const char *_ptr, register FILE *_fp)
{
  unsigned int i, len;

  len = strlen(_ptr);

  for(i=0 ; i<len ; i++)
  {
      uartWriteByte((unsigned char) _ptr[i]);
  }

  return len;
}

